        LOCAL_ARM_NEON  := true       
endif # TARGET_ARCH_ABI == armeabi-v7a

ifeq ($(TARGET_ARCH_ABI),arm64-v8a)
        LOCAL_ARM_NEON  := true
endif # TARGET_ARCH_ABI == arm64-v8a


